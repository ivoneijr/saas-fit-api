{
  "name": "api",
  "version": "1.0.0",
  "main": "src/server.js",
  "author": "@ivoneijr",
  "license": "MIT",
  "scripts": {
    "start": "pm2 start process.yml",
    "dev": "nodemon src/server.js --ignore __test__",
    "db:migrate-make": "knex --knexfile=src/config/knexfile.js migrate:make ",
    "db:migrate": "knex --knexfile=src/config/knexfile.js migrate:latest",
    "db:migrate-rollback": "knex --knexfile=src/config/knexfile.js migrate:rollback",
    "db:reset": "knex --knexfile=src/config/knexfile.js migrate:rollback --all && knex --knexfile=src/config/knexfile.js migrate:latest",
    "db:seed": "knex --knexfile=src/config/knexfile.js seed:run",
    "db:seed-make": "knex --knexfile=src/config/knexfile.js seed:make ",
    "pretest": "set NODE_ENV=test&& knex --knexfile=src/config/knexfile.js migrate:latest",
    "test": "set NODE_ENV=test&& jest",
    "posttest": "set NODE_ENV=test&& knex --knexfile=src/config/knexfile.js migrate:rollback --all"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.2",
    "nodemon": "2.0.2",
    "prettier": "2.0.4",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "axios": "0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "1.19.0",
    "bookshelf": "^1.1.1",
    "checkenv": "1.2.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "jest": "^26.4.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "lodash": "4.17.15",
    "moment": "^2.26.0",
    "opentok": "^2.10.0",
    "pg": "^8.2.1",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "uuidv4": "^6.2.3",
    "winston": "^3.2.1",
    "winston-loggly-bulk": "^3.1.0"
  }
}
